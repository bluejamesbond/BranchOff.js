{
  "name": "branch-off",
  "version": "1.31.0",
  "description": "Auto branch-deployment with Github webhooks",
  "main": "app.js",
  "dependencies": {
    "async": "^1.5.2",
    "body-parser": "^1.14.2",
    "express": "^4.13.4",
    "extend": "^3.0.0",
    "github-webhook-handler": "git://github.com/bluejamesbond/github-webhook-handler",
    "jade": "^1.11.0",
    "pm2": "latest",
    "pmx": "beta",
    "scribe-js": "git://github.com/bluejamesbond/Scribe.js.git#dev",
    "shelljs": "^0.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bluejamesbond/BranchOff.js"
  },
  "config": {
    "port": 5000,
    "socketPort": 5999,
    "start": 3000,
    "end": 4000,
    "defaultRepo": "",
    "dir": "",
    "maxInstances": -1
  },
  "apps": [
    {
      "merge_logs": true,
      "max_memory_restart": "400M",
      "script": "app.js"
    }
  ],
  "author": "Mathew Kurian",
  "license": "MIT"
}